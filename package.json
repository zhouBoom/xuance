{
  "name": "udc-xuance",
  "version": "1.0.11",
  "private": true,
  "type": "commonjs",
  "main": "dist/src/main.js",
  "description": "玄策",
  "author": "杨艺韬<yangyitao2@tal.com>",
  "scripts": {
    "start": "concurrently \"npm:main:start\" \"npm --prefix client run start\"",
    "build": "npm run build:main && npm run build:client",
    "build:main": "tsc -p tsconfig.json",
    "build:client": "npm --prefix client run build",
    "main:start": "electron .",
    "watch:main": "tsc -p tsconfig.json --watch",
    "dev": "*不要用这个了，要用dev:local*cross-env**NODE_ENV=development**concurrently**\"npm run watch:main\"**\"npm**--prefix**client**run**start\"**\"npm:main:start\"",
    "dev:local": "cross-env NODE_ENV=sandbox NODE_ENV_FLAG=sandbox node ./build/dev.js",
    "dev:online": "cross-env NODE_ENV=online NODE_ENV_FLAG=online node ./build/dev.js",
    "dist:win": "electron-builder --win",
    "build:sandbox": "cross-env NODE_ENV=sandbox node ./build/sandbox.js",
    "build:online": "cross-env NODE_ENV=online node ./build/online.js",
    "build:release": "cross-env NODE_ENV=online node ./build/release.js",
    "electron:rebuild": "electron-rebuild"
  },
  "dependencies": {
    "@types/md5": "^2.3.5",
    "async": "^3.2.6",
    "axios": "^1.7.7",
    "better-queue": "^3.8.12",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "electron-store": "^10.0.0",
    "express": "^4.17.1",
    "fastify": "^3.22.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "got": "^14.4.5",
    "html-webpack-plugin": "^5.6.3",
    "https-proxy-agent": "^7.0.6",
    "is-arrayish": "^0.3.2",
    "is-type-of": "^2.2.0",
    "koffi": "^2.11.0",
    "md5": "^2.3.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.94.0",
    "playwright": "^1.52.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.29",
    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.5.13",
    "concurrently": "^6.2.0",
    "electron": "25.9.8",
    "electron-builder": "latest",
    "electron-rebuild": "^3.2.9",
    "javascript-obfuscator": "^4.1.1",
    "moment": "^2.30.1",
    "style-loader": "^4.0.0",
    "typescript": "^4.5.0"
  }
}
